{
  "name": "zuna-server",
  "version": "1.0.0",
  "description": "Backend server for Zuna Simple Decor",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo \"Backend build completed\"",
    "seed": "node scripts/seedDatabase.js",
    "seed-services": "node scripts/seedServices.js",
    "seed-legacy": "node scripts/seedProducts.js",
